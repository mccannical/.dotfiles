#!/bin/bash
#Generated by JetBrains Toolbox 2.5.1.34629 at 2024-10-30T09:39:12

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/GoLand 2024.3 EAP.app/Contents/MacOS/goland" $wait --args "${intellij_args[@]}"