#!/bin/bash
#Generated by JetBrains Toolbox 2.4.2.32922 at 2024-10-11T09:30:24.476321

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/jesse.mccann/Applications/PyCharm Professional Edition.app/Contents/MacOS/pycharm" $wait --args "${intellij_args[@]}"